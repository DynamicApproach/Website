{
  "name": "thomaslloyd-jones",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "eslint": "eslint .",
    "fix": "eslint --fix . && prettier --write .",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci",
    "test:debug": "node --inspect server.js",
    "test:update": "jest --updateSnapshot",
    "test:verbose": "jest --verbose",
    "clean": "rm -rf .next/ && rm -rf coverage/"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^0.2.0",
    "@huggingface/inference": "^2.5.2",
    "@react-three/cannon": "^6.5.2",
    "@react-three/drei": "^9.78.1",
    "@react-three/fiber": "^8.9.1",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "framer-motion": "^10.12.16",
    "framer-motion-3d": "^10.12.16",
    "google-auth-library": "^8.8.0",
    "locomotive-scroll": "^4.1.4",
    "markmap-lib": "^0.14.4",
    "markmap-view": "^0.15.0",
    "next": "^13.4.11",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-locomotive-scroll": "^0.2.2",
    "sharp": "^0.32.1",
    "three": "^0.154.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@next/eslint-plugin-next": "^13.4.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.4",
    "@types/styled-components": "^5.1.25",
    "@types/three": "^0.152.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.11",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "styled-components": "^6.0.7",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
